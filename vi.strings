"T_MAP_CANALGRANDE_DESC" = "Một đầm phá đầy những hòn đảo nhỏ, cho phép tạo ra một thành phố được bao phủ bởi các cây cầu và kênh đào.";
"T_MAP_COASTALISLAND_DESC" = "Có một hòn đảo bình dị nằm trong vịnh này. Các ngươi có thể xây dựng một ngôi làng tầm cỡ thực sự ở nơi hẻo lánh này không?";
"T_MAP_COASTSANDCLIFFS_DESC" = "Lâu đài của các ngươi nhìn ra đại dương từ một vách đá cao. Một thành phố ven biển xinh đẹp có thể được xây dựng trên các cao nguyên xung quanh.";
"T_MAP_COASTSANDCLIFFS_NAME" = "Bờ biển và vách đá";
"T_MAP_DESC_10" = "Một thành phố tráng lệ có thể được xây dựng trong thung lũng thịnh vượng này.";
"T_MAP_DESC_11" = "Đây là bản đồ thử nghiệm để kiểm tra hiệu suất trên các thiết bị khác nhau. Một ngôi làng thực sự lớn được phân bố trên bản đồ.";
"T_MAP_DESC_12" = "Những dòng sông róc rách trên bản đồ này mời người dân đi bơi và câu cá.";
"T_MAP_DESC_13" = "Bên dưới cao nguyên râm mát, một ngôi làng nhỏ có thể nở rộ thành một thành phố rộng lớn.";
"T_MAP_DESC_14" = "Vương quốc đang bị hạn hán hoành hành - dòng sông gần như đã khô cạn! Sẽ là một nhiệm vụ khó khăn để canh tác các cánh đồng và cung cấp lương thực cho người dân của các ngươi.";
"T_MAP_DESC_15" = "Dựng một trạm buôn bán thịnh vượng trên cao nguyên. Các giao dịch sinh lợi với các thương nhân và hiệp sĩ tự do đang chờ đợi bạn.";
"T_MAP_DESC_16" = "Dựng một thuộc địa khai thác mỏ và cung cấp cho một tượng đài khối lượng lớn tài nguyên.";
"T_MAP_DESC_17" = "Hãy chuẩn bị cho một đám cưới hoàng gia lớn.";
"T_MAP_DESC_18" = "Một thành phố lớn có thể được xây dựng trên lòng sông khô cạn này. Tuy nhiên, bạn sẽ không tìm thấy nhiều nước ở đây đâu.";
"T_MAP_DESC_19" = "Ở rìa vùng đầm lầy là một cao nguyên nhỏ - một nơi lý tưởng cho một thành phố nhỏ.";
"T_MAP_DESC_2" = "Địa điểm yên tĩnh này trên bờ biển là một môi trường lý tưởng để thành lập một ngôi làng tuyệt vời.";
"T_MAP_DESC_20" = "Một vùng núi hẻo lánh ở giữa hư không. Không có nước, nhưng có vô số mỏ quặng. Một nơi lý tưởng để kiếm bộn tiền!";
"T_MAP_DESC_2016_01" = "Đức Vua cần rất nhiều ván gỗ để mở rộng hạm đội tàu của mình.\nHãy tìm hiểu lý do tại sao không còn các chuyến hàng ván gỗ đến nữa và thành lập một phường hội thương nhân.";
"T_MAP_DESC_2016_02" = "Khu vực xinh đẹp này là một nơi lý tưởng để xây dựng một thành phố lớn. Các ngọn đồi rộng lớn cung cấp đủ không gian cho vô số tòa nhà.";
"T_MAP_DESC_2016_03" = "Một thành phố nhỏ đã phát triển thành một đô thị lớn. Hãy chăm sóc các nhu cầu và mong muốn của người dân trong thời gian quy định.";
"T_MAP_DESC_2016_04" = "Thay đổi khéo léo nghề nghiệp của một số ít người dân để xây dựng một khu chợ và một quán trọ.";
"T_MAP_DESC_2016_05" = "Hãy lên kế hoạch tốt cho thành phố của bạn, vì giữa các thác nước có địa hình khó khăn và ít không gian để xây dựng.";
"T_MAP_DESC_2016_06" = "Tuyết lở đe dọa ngôi làng trong khu vực này. Bất chấp những nguy hiểm, hãy xây dựng một thị trấn nhỏ để có thể tận dụng các mỏ quặng phong phú.";
"T_MAP_DESC_2016_08" = "Xây dựng thành phố của bạn trong hẻm núi này. Bạn có thể tìm thấy mọi thứ mình cần, nhưng không phải tất cả các khu vực đều dễ tiếp cận.";
"T_MAP_DESC_2016_09" = "Vượt qua con sông lớn để tiếp cận các tài nguyên quan trọng mà bạn cần cho thành phố của mình.";
"T_MAP_DESC_2016_10" = "Tích lũy một lượng lớn đồng Taler. Tuy nhiên, hãy cẩn thận, rất nhiều bọn cướp đang lẩn trốn trong khu vực. Ngoài ra, những người dân bất hạnh không thích trả thuế.";
"T_MAP_DESC_21" = "Dựng một ngôi làng trên một hòn đảo xinh đẹp, ẩn mình. Hãy tận hưởng không khí biển trong lành!";
"T_MAP_DESC_21_NOBANDITS" = "Dựng một ngôi làng trên một hòn đảo xinh đẹp, ẩn mình. Hãy tận hưởng không khí biển trong lành!";
"T_MAP_DESC_22" = "Đưa ngôi làng của bạn vượt qua mùa đông khắc nghiệt.";
"T_MAP_DESC_23" = "Trên bình nguyên đóng băng này, bạn phải hành động nhanh chóng để ngôi làng của bạn vượt qua mùa đông một cách an toàn.";
"T_MAP_DESC_24" = "Hỗ trợ Đức Vua đóng một hạm đội và giải quyết tình trạng bất ổn trong làng của bạn.";
"T_MAP_DESC_25" = "Sử dụng rừng xung quanh một cách khôn ngoan để xây dựng thành phố của bạn. Nước rất khan hiếm trong khu vực này, vì vậy đánh bắt cá không phải là một lựa chọn tốt để cung cấp cho người dân của bạn.";
"T_MAP_DESC_26" = "Thay mặt Đức Vua tham gia một cuộc cá cược, xây dựng thành phố của bạn trước Đại Công tước. Bạn có rất ít thời gian để hoàn thành tất cả các nhiệm vụ. Đừng làm Đức Vua thất vọng!";
"T_MAP_DESC_27" = "Thật khó để xây dựng trên diện rộng ở vùng đầm lầy này. Hãy sử dụng không gian cho trước một cách khôn ngoan để xây dựng thành phố của bạn.";
"T_MAP_DESC_28" = "Một thung lũng đá, nơi có rất nhiều tài nguyên – nhưng diện tích xây dựng bị hạn chế.";
"T_MAP_DESC_29" = "Một nhóm đảo nhỏ mà việc định cư là một thách thức.";
"T_MAP_DESC_29_NOBANDITS" = "Một nhóm đảo nhỏ mà việc định cư là một thách thức.";
"T_MAP_DESC_3" = "Đây là bản đồ hướng dẫn. Tại đây, bạn sẽ làm quen với thế giới của Townsmen.";
"T_MAP_DESC_30" = "Xây dựng một thành phố xung quanh pháo đài trên núi của bạn. Mùa đông dài khiến việc cung cấp lương thực trở nên khó khăn ở đây.";
"T_MAP_DESC_31" = "Đức Vua có kế hoạch thị sát lãnh thổ của mình. Hãy nhanh chóng chuẩn bị mọi thứ cho sự xuất hiện của ngài.";
"T_MAP_DESC_32" = "Sinh nhật của Đức Vua đang đến gần và bạn đang tìm kiếm loại đồ uống hoàn hảo để làm quà tặng cho ngài - có lẽ các tu sĩ sẽ biết nhiều hơn…";
"T_MAP_DESC_33" = "Đức Vua đang mắc một căn bệnh bí ẩn. Tùy thuộc vào các ngươi để chữa trị cho ngài và tìm ra nơi mà căn bệnh đó đến.";
"T_MAP_DESC_34" = "Chiến tranh đang nắm giữ đất nước một cách chắc chắn. Ở xa tiền tuyến, bạn phải xây dựng một nền kinh tế thịnh vượng và hỗ trợ quân đội của Đức Vua. Tuy nhiên, người dân cũng có những mong muốn của riêng mình.";
"T_MAP_DESC_35" = "Đức Vua muốn chuẩn bị cho những thời điểm khó khăn. Thành phố của các ngươi được giao nhiệm vụ trồng lúa mì cần thiết.";
"T_MAP_DESC_36" = "Học hỏi những điều phức tạp của chiến tranh từ một cựu chiến binh.";
"T_MAP_DESC_37" = "Đáp ứng những mong muốn của người vợ khắt khe của bạn và đối phó với một tên cướp tống tiền.";
"T_MAP_DESC_4" = "Lâu đài của bạn nằm trên một hòn đảo nhỏ. Hãy sử dụng các khu vực bờ biển xung quanh để xây dựng một nền kinh tế thịnh vượng.";
"T_MAP_DESC_6" = "Tại đây, bạn có thể xây dựng một ngôi làng nhỏ trên bờ một hồ nước tuyệt đẹp.";
"T_MAP_DESC_7" = "Thành phố của bạn đã được Đức Vua chọn để tổ chức giải đấu hiệp sĩ lớn nhất trong khu vực!";
"T_MAP_DESC_8" = "Mùa đông đang ở trước cửa thành phố của bạn! Hãy giúp người dân chuẩn bị cho mùa lạnh nhất trong năm!";
"T_MAP_DESC_9" = "Hỗ trợ vợ của bạn và đưa nền kinh tế trở lại thịnh vượng trong một ngôi làng tồi tàn!";
"T_MAP_DESC_APOCALYPSE" = "Thành phố đang phải chịu một lời nguyền kỳ lạ. Bệnh tật và thảm họa đang gây áp lực lên người dân. Hãy tìm cách phá bỏ lời nguyền càng sớm càng tốt. Bất kể giá nào…";
"T_MAP_DESC_FESTIVITIES" = "Đức Vua đã kết hôn lần nữa. Để tôn vinh vị Hoàng hậu mới, các lễ hội lớn sẽ được tổ chức trên khắp đất nước. Tuy nhiên, việc lập kế hoạch và thực hiện là khó khăn. Đây sẽ là một cuộc chạy đua với thời gian…";
"T_MAP_DESC_ICYHOLD" = "Nằm trên đỉnh một ngọn núi dốc đứng là pháo đài băng giá khó tiếp cận. Từ đây, bạn có thể nhìn bao quát vùng lãnh nguyên trơ trụi xung quanh. Mùa đông ở đây rất dài!";
"T_MAP_DIPLOMATICVISIT_DESC" = "Nữ hoàng của một đất nước thân thiện đang đến thăm thành phố. Các thỏa thuận thương mại sinh lợi sẽ được ký kết, nhưng có một số vấn đề về ngôn ngữ.";
"T_MAP_DIPLOMATICVISIT_NAME" = "Chuyến thăm ngoại giao";
"T_MAP_DIVIDEDSHORELINE_DESC" = "Một ngọn núi hùng vĩ chia cắt bờ biển này thành hai khu vực. Ngoài ra, một hòn đảo nhỏ ngoài khơi có thể được khai thác.";
"T_MAP_DIVIDEDSHORELINE_NAME" = "Bờ biển bị chia cắt";
"T_MAP_FRIGYTHKINGDOM_DESC" = "Nữ hoàng Frigyth cần một Thống đốc để quản lý khu vực này của vương quốc. Hãy xây dựng một thành phố tráng lệ ở vùng đất mới này.";
"T_MAP_FRIGYTHKINGDOM_NAME" = "Vương quốc Frigyth";
"T_MAP_FROZENFRONT_DESC" = "Cuộc chiến cuối cùng đã tàn phá phần lớn ngôi làng. Trận chiến lớn đã diễn ra ở nơi này. Giờ là lúc xây dựng lại ngôi làng.\n\nMùa đông luôn diễn ra trên bản đồ này!";
"T_MAP_HIGHLANDS_DESC" = "Vùng đất này được bao phủ bởi núi và đồi. Khung cảnh rất tuyệt vời ngay khi bạn lên đến đỉnh núi.";
"T_MAP_HIGHLANDS_INTRO" = "Chào mừng đến với vùng Cao nguyên, thưa Thống đốc. Khu vực này rải rác những điều bí ẩn. Tôi chắc chắn rằng những vùng đất này sẽ sớm tiết lộ tất cả bí mật của chúng cho các ngài nếu các ngài chăm sóc người dân một cách chu đáo và cẩn thận.";
"T_MAP_HIGHLANDS_NAME" = "Cao nguyên";
"T_MAP_INSANECHOICES_DESC" = "Các ngươi đã được gọi đến một ngôi làng nơi Công tước dường như bị điên. Người dân không còn trả thuế nữa.\nMỗi quyết định của các ngươi có thể dẫn đến một kết thúc khác của câu chuyện…";
"T_MAP_INSANECHOICES_NAME" = "Những Quyết định Điên Rồ";
"T_MAP_LEGACY_DESC" = "Đức Vua có một nhiệm vụ mới dành cho các ngươi. Ngài đã khá lớn tuổi và lo sợ rằng mình sẽ rời khỏi thế giới này mà không để lại di sản. Ngài muốn truyền lại những bài học mà ngài đã học được với tư cách là một vị vua cho các thế hệ tương lai.";
"T_MAP_LEGACY_NAME" = "Di sản";
"T_MAP_LOWLANDS_DESC" = "Đồng bằng rộng lớn và nhiều vùng đất xây dựng trải dài đến tận chân trời. Một thành phố thực sự lớn có thể được xây dựng ở đây.";
"T_MAP_LOWLANDS_NAME" = "Vùng đất thấp";
"T_MAP_MARSH_DESC" = "Nhiều vùng đầm lầy ẩm ướt đặc trưng cho khu vực này và góp phần vào sự đa dạng sinh học của đất nước.";
"T_MAP_MARSH_INTRO" = "Thưa ngài, hãy nhìn vào sự cân bằng hoàn hảo giữa đất và nước này. Tôi rất vui khi được chào đón ngài đến địa điểm yên tĩnh này.\n\nNgười dân ở đây sẽ sớm chứng kiến cách ngài giúp khu vực này đạt đến tầm vóc thực sự!";
