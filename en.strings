"T_MAP_CANALGRANDE_DESC" = "Một đầm phá với nhiều hòn đảo nhỏ là nơi hoàn hảo để xây dựng một thị trấn được kết nối bằng những cây cầu và đi qua bởi những con kênh.";
"T_MAP_COASTALISLAND_DESC" = "Trong vịnh này có một hòn đảo bình dị. Bạn có thể thành lập một thị trấn thực sự quan trọng ở địa điểm xa xôi này không?";
"T_MAP_COASTSANDCLIFFS_DESC" = "Lâu đài nhìn ra đại dương từ một vách đá cao. Trên các cao nguyên xung quanh, bạn có thể xây dựng một thị trấn ven biển xinh đẹp.";
"T_MAP_COASTSANDCLIFFS_NAME" = "Vách đá và Bờ biển";
"T_MAP_DESC_10" = "Thung lũng tươi tốt này hoàn toàn phù hợp để xây dựng một thị trấn thịnh vượng.";
"T_MAP_DESC_11" = "Bản đồ này được tạo ra để kiểm tra hiệu suất trên tất cả các thiết bị. Một thị trấn thực sự lớn đang trải dài khắp bản đồ.";
"T_MAP_DESC_12" = "Làn nước gợn sóng của các con sông trong vùng này mời gọi Dân làng đi câu cá hoặc bơi lội trong làn nước trong vắt.";
"T_MAP_DESC_13" = "Bên dưới cao nguyên râm mát, một ngôi làng nhỏ có thể phát triển thành một thị trấn lớn.";
"T_MAP_DESC_14" = "Vương quốc đang phải chịu hạn hán. Dòng sông đã cạn khô. Điều này khiến việc canh tác đất đai và nuôi sống Dân làng của bạn trở thành một nhiệm vụ đầy thách thức.";
"T_MAP_DESC_15" = "Thành lập một trạm giao dịch thịnh vượng trên cao nguyên. Những giao dịch béo bở với thương nhân và hiệp sĩ đang chờ đợi bạn.";
"T_MAP_DESC_16" = "Thành lập một thuộc địa khai thác mỏ và cung cấp tài nguyên cho việc xây dựng một nhà thờ lớn hoành tráng.";
"T_MAP_DESC_17" = "Chuẩn bị cho một đám cưới hoàng gia lớn.";
"T_MAP_DESC_18" = "Xây dựng một thị trấn lớn gần lòng sông cạn khô này, nhưng hãy nhớ rằng nước là một mặt hàng khan hiếm trong khu vực này.";
"T_MAP_DESC_19" = "Có một cao nguyên nhỏ gần rìa đầm lầy. Một nơi lý tưởng cho một thị trấn.";
"T_MAP_DESC_2" = "Nơi yên tĩnh này ở bờ biển là địa điểm lý tưởng để thành lập làng của bạn.";
"T_MAP_DESC_20" = "Một vùng núi biệt lập giữa hư không. Không có nước, nhưng có vô số mạch quặng. Nơi hoàn hảo để làm giàu!";
"T_MAP_DESC_2016_01" = "Đức Vua cần rất nhiều ván gỗ để đóng một hạm đội lớn. Tìm hiểu lý do tại sao việc giao ván gỗ bị dừng lại và thành lập một Hội thương mại.";
"T_MAP_DESC_2016_02" = "Vùng đất xinh đẹp này là nơi hoàn hảo để xây dựng một thành phố lớn. Những ngọn đồi rộng lớn cung cấp không gian rộng rãi cho vô số công trình.";
"T_MAP_DESC_2016_03" = "Một thị trấn nhỏ đã trở thành một đô thị lớn. Hoàn thành mong muốn và nhu cầu của Dân làng trong thời gian quy định.";
"T_MAP_DESC_2016_04" = "Thay đổi công việc của các Dân làng một cách khôn ngoan để xây dựng một khu chợ và một quán rượu.";
"T_MAP_DESC_2016_05" = "Lập kế hoạch thị trấn của bạn thật tốt, không gian cho các công trình rất khan hiếm giữa các thác nước và địa hình có thể khó đi qua.";
"T_MAP_DESC_2016_06" = "Có những trận tuyết lở lớn trong khu vực này. Bên cạnh đó, bạn nên thành lập một thị trấn nhỏ và khai thác các mỏ quặng phong phú.";
"T_MAP_DESC_2016_08" = "Xây dựng thị trấn của bạn trong hẻm núi. Bạn có thể tìm thấy mọi thứ mình cần, nhưng một số khu vực khó tiếp cận.";
"T_MAP_DESC_2016_09" = "Băng qua sông để đến các tài nguyên cần thiết cho thành phố của bạn.";
"T_MAP_DESC_2016_10" = "Tiết kiệm một đống tiền xu. Nhưng hãy cẩn thận, bọn cướp đã lập sào huyệt trong khu vực. Ngoài mối đe dọa từ bọn cướp, những dân làng không vui sẽ không trả thuế.";
"T_MAP_DESC_21" = "Thành lập một ngôi làng trên một hòn đảo xinh đẹp, ẩn mình. Hãy tận hưởng không khí biển trong lành!";
"T_MAP_DESC_21_NOBANDITS" = "Thành lập một ngôi làng trên một hòn đảo xinh đẹp, ẩn mình. Hãy tận hưởng không khí biển trong lành!";
"T_MAP_DESC_22" = "Đưa thị trấn của bạn vượt qua mùa đông khắc nghiệt.";
"T_MAP_DESC_23" = "Trên đồng bằng băng giá này, bạn phải xây dựng một thị trấn hoạt động hiệu quả rất nhanh chóng hoặc chết đói trong mùa đông.";
"T_MAP_DESC_24" = "Hỗ trợ Đức Vua trong nỗ lực xây dựng một hạm đội tàu lớn, đồng thời kiểm soát tình trạng bất ổn ngày càng tăng trong thị trấn của bạn.";
"T_MAP_DESC_25" = "Tận dụng các khu rừng xung quanh trong khu vực này khi cố gắng thành lập thị trấn của bạn. Nước ở đây khan hiếm, vì vậy không thể dựa vào việc đánh bắt cá.";
"T_MAP_DESC_26" = "Đức Vua đã tham gia một vụ cá cược và nhiệm vụ của bạn là xây dựng một thành phố lớn trước khi đại công tước làm được. Bạn không có nhiều thời gian để hoàn thành tất cả các nhiệm vụ. Đừng làm ngài thất vọng!";
"T_MAP_DESC_27" = "Các khu vực đầm lầy khiến việc tìm kiếm nền đất xây dựng phù hợp trở nên khó khăn. Hãy chắc chắn sử dụng hiệu quả nền đất xây dựng có sẵn!";
"T_MAP_DESC_28" = "Một thung lũng đá chứa rất nhiều tài nguyên, nhưng không gian cho các công trình lại khan hiếm.";
"T_MAP_DESC_29" = "Một nhóm đảo nhỏ. Thuộc địa hóa chúng sẽ là một thách thức.";
"T_MAP_DESC_29_NOBANDITS" = "Một nhóm đảo nhỏ. Thuộc địa hóa chúng sẽ là một thách thức.";
"T_MAP_DESC_3" = "Đây là bản đồ hướng dẫn. Tại đây bạn sẽ được giới thiệu về thế giới của Townsmen.";
"T_MAP_DESC_30" = "Xây dựng một thị trấn xung quanh pháo đài trên núi của bạn. Nhưng hãy cẩn thận, một mùa đông dài đang đến.";
"T_MAP_DESC_31" = "Đức Vua sẽ đến thăm các lãnh địa của mình. Hãy chuẩn bị cho sự xuất hiện của ngài.";
"T_MAP_DESC_32" = "Sinh nhật của Đức Vua sắp đến. Bây giờ bạn tìm kiếm loại bia hoàn hảo làm quà tặng. Có lẽ các tu sĩ tại tu viện biết nhiều hơn...";
"T_MAP_DESC_33" = "Đức Vua mắc một căn bệnh bí ẩn. Nhiệm vụ của bạn là chữa khỏi cho ngài và tìm ra nguồn gốc của căn bệnh.";
"T_MAP_DESC_34" = "Chiến tranh đang bao trùm vùng đất. Ở xa mặt trận, bạn phải thành lập một thị trấn thịnh vượng và hỗ trợ quân đội của Đức Vua.";
"T_MAP_DESC_35" = "Đức Vua muốn chuẩn bị cho những thời điểm khó khăn và thị trấn của bạn đã được chọn để cung cấp hàng hóa cần thiết.";
"T_MAP_DESC_36" = "Học hỏi các chi tiết về chiến tranh từ một cựu chiến binh.";
"T_MAP_DESC_37" = "Hoàn thành mong muốn của vợ bạn trong khi đối phó với một tên cướp tống tiền.";
"T_MAP_DESC_4" = "Lâu đài của bạn nằm trên một hòn đảo nhỏ trên sông. Sử dụng bờ sông xung quanh một cách khôn ngoan và xây dựng một nền kinh tế thịnh vượng.";
"T_MAP_DESC_6" = "Xây dựng một ngôi làng nhỏ bên bờ hồ xinh đẹp.";
"T_MAP_DESC_7" = "Thị trấn của bạn đã được Đức Vua chọn để tổ chức giải đấu thương lớn nhất khu vực!";
"T_MAP_DESC_8" = "Mùa đông đang đến! Giúp Dân làng của bạn chuẩn bị cho mùa băng giá!";
"T_MAP_DESC_9" = "Hỗ trợ vợ bạn và làm cho nền kinh tế của một ngôi làng đổ nát phát triển trở lại!";
"T_MAP_DESC_APOCALYPSE" = "Thị trấn đang phải chịu một lời nguyền. Những căn bệnh và thảm họa kỳ lạ đang đè nặng lên dân chúng. Nhiệm vụ của bạn là tìm cách giải lời nguyền càng sớm càng tốt. Bất kể phải trả giá nào…";
"T_MAP_DESC_FESTIVITIES" = "Đức Vua đã tái hôn. Để tôn vinh người vợ mới, bạn được lệnh tổ chức một số lễ hội theo mùa. Tuy nhiên, việc lập kế hoạch và thực hiện có thể khó khăn. Đó sẽ là một cuộc chạy đua với thời gian…";
"T_MAP_DESC_ICYHOLD" = "Cao trên một vách đá là pháo đài băng giá. Khó tiếp cận nhưng một khi đã leo lên, người ta có thể nhìn xa vào khu vực xung quanh của lãnh nguyên lạnh giá. Mùa đông rất dài ở khu vực này!";
"T_MAP_DIPLOMATICVISIT_DESC" = "Nữ hoàng của một quốc gia thân thiện đến thăm thành phố. Các thỏa thuận thương mại béo bở sắp được ký kết, nhưng có một rào cản ngôn ngữ cần phải vượt qua trước tiên.";
"T_MAP_DIPLOMATICVISIT_NAME" = "Chuyến thăm ngoại giao";
"T_MAP_DIVIDEDSHORELINE_DESC" = "Một ngọn núi hùng vĩ đang chia cắt vùng đất của bạn. Hơn nữa, bạn có thể chinh phục một hòn đảo không xa lâu đài của mình.";
"T_MAP_DIVIDEDSHORELINE_NAME" = "Bờ biển chia cắt";
"T_MAP_FRIGYTHKINGDOM_DESC" = "Nữ hoàng Frigyth cần một thống đốc mới để quản lý phần này của Vương quốc của bà. Xây dựng một thị trấn tráng lệ ở vùng đất mới này.";
"T_MAP_FRIGYTHKINGDOM_NAME" = "Vương quốc Frigyth";
"T_MAP_FROZENFRONT_DESC" = "Cuộc chiến cuối cùng đã khiến phần lớn thị trấn trở thành đống đổ nát. Những tàn tích phủ đầy tuyết của thị trấn giờ đây đánh dấu nơi diễn ra trận chiến lớn của các đội quân. Bây giờ là lúc để xây dựng lại thị trấn.\n\nĐây là bản đồ chỉ có mùa đông!";
"T_MAP_HIGHLANDS_DESC" = "Vùng đất này bị cắt ngang bởi núi và đồi. Khung cảnh thật tuyệt vời khi bạn lên đến đỉnh.";
"T_MAP_HIGHLANDS_INTRO" = "Chào mừng đến cao nguyên, thưa thống đốc. Vùng đất này đầy rẫy những bí ẩn. Tôi chắc chắn rằng, nếu ngài chú ý và quan tâm đến cư dân, vùng đất sẽ sớm tiết lộ tất cả bí mật của nó cho ngài.";
"T_MAP_HIGHLANDS_NAME" = "Cao nguyên";
"T_MAP_INSANECHOICES_DESC" = "Bạn được gọi đến một ngôi làng nơi Công tước dường như bị điên. Dân làng từ chối trả bất kỳ khoản thuế nào!\nMỗi lựa chọn bạn đưa ra sẽ dẫn bạn đến một kết thúc khác nhau.";
"T_MAP_INSANECHOICES_NAME" = "Lựa chọn Điên rồ";
"T_MAP_LEGACY_DESC" = "Đức Vua có một nhiệm vụ mới cho bạn. Ngài đang già đi và sợ rằng mình sẽ rời khỏi thế giới này mà không để lại di sản. Một di sản sẽ truyền lại những bài học ngài đã học được khi làm Vua cho các thế hệ tương lai.";
"T_MAP_LEGACY_NAME" = "Di sản";
"T_MAP_LOWLANDS_DESC" = "Những đồng bằng rộng lớn và rất nhiều không gian xây dựng trải dài đến tận chân trời. Ở đây có thể xây dựng một thành phố thực sự lớn.";
"T_MAP_LOWLANDS_NAME" = "Đồng bằng";
"T_MAP_MARSH_DESC" = "Trong khu vực này, nhiều đồng cỏ ẩm ướt góp phần vào sự đa dạng sinh học của vùng đất.";
"T_MAP_MARSH_INTRO" = "Thưa Lãnh chúa, hãy nhìn sự cân bằng hoàn hảo giữa đất và nước này. Tôi rất vui được chào đón ngài đến nơi yên tĩnh này.\n\nNhững cư dân ở đây sẽ sớm chứng kiến những nỗ lực của ngài trong việc giúp khu vực này đạt đến sự vĩ đại thực sự!";
