"T_RANDOM_EVENT_HARVEST_HELPERS_TITLE" = "Người thu hoạch";
"T_RANDOM_EVENT_INVESTMENT_CHOICE_POPUP_01_REPLY_01" = "Đầu tư tiền.";
"T_RANDOM_EVENT_INVESTMENT_CHOICE_POPUP_01_REPLY_02" = "Thay vào đó, đầu tư tài nguyên.";
"T_RANDOM_EVENT_INVESTMENT_CHOICE_POPUP_01_REPLY_03" = "Tôi có thể cho ngài cái gì khác?";
"T_RANDOM_EVENT_INVESTMENT_CHOICE_POPUP_01_REPLY_04" = "Không, tôi không biết. Tôi rút lui.";
